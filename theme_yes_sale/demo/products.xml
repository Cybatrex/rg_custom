    <!-- Create new categories -->
    <record id="clothes" model="product.public.category">
        <field name="name">Wedding Clothes</field>
    </record>
    <record id="wedding_dress" model="product.public.category">
        <field name="parent_id" ref="clothes"/>
        <field name="name">Wedding Dresses</field>
    </record>
    <record id="shoes" model="product.public.category">
        <field name="parent_id" ref="clothes"/>
        <field name="name">Wedding Shoes</field>
    </record>

    <record id="jewelry" model="product.public.category">
        <field name="name">Jewelry</field>
    </record>
    <record id="ring" model="product.public.category">
