        <field name="name">Wedding Rings</field>
    </record>
    <record id="necklaces" model="product.public.category">
        <field name="parent_id" ref="jewelry"/>
        <field name="name">Necklaces</field>
    </record>

    <record id="accessories" model="product.public.category">
        <field name="name">Accessories</field>
    </record>
    <record id="cake" model="product.public.category">
        <field name="parent_id" ref="accessories"/>
        <field name="name">Wedding Cakes</field>
    </record>

